Gizmo {
 help "1. Choose the way the to create the chromatic aberration (LensDistortion will only work in NukeX!)\n\n2. Adjust Settings in corresponding section\n\n3. Additional transform operation can be added to both LensDistortion and Scale\n\n\nPlay around with the red & green channel, e.g. use only green or red with a bigger value than green to create different coloured effects."
 addUserKnob {20 User l "Chromatic Aberration"}
 addUserKnob {4 distortion l "kind of distortion" M {"Lens Distortion" Scale}}
 distortion Scale
 addUserKnob {26 ""}
 addUserKnob {20 LensDistortion n 1}
 addUserKnob {41 distortion1 l red T LensDistortion2.distortion1}
 addUserKnob {41 distortion1_1 l green T LensDistortion1.distortion1}
 addUserKnob {20 endGroup n -1}
 addUserKnob {26 ""}
 addUserKnob {20 Scale n 1}
 addUserKnob {14 red_scale_scale l red R 0.1 10}
 red_scale_scale 1.007
 addUserKnob {14 green_scale_scale l green R 0.1 10}
 green_scale_scale 1.003
 addUserKnob {14 blue_scale_scale l blue R 0.1 10}
 blue_scale_scale 1
 addUserKnob {20 endGroup_1 l endGroup n -1}
 addUserKnob {26 ""}
 addUserKnob {20 transform l Transform n 1}
 addUserKnob {41 red_pos l red T trans_red.translate}
 addUserKnob {41 green_pos l green T trans_green.translate}
 addUserKnob {41 blue_pos l blue T trans_blue.translate}
 addUserKnob {26 "" +STARTLINE}
 addUserKnob {26 copyright l "" +STARTLINE T "Â© Claus Steinmassl 2012\nwww.claus-steinmassl.com"}
 addUserKnob {20 endGroup_2 l endGroup n -1}
}
